<table class="table table-striped table-hover">
<thead>
  <tr>
    <th>標題</th>
    <th>修改日期</th>
  </tr>
</thead>
<tbody>
  <% @notes.each do |note| %>
  <tr>
    <td><%= link_to note.title, :controller => 'note', :action => 'show', :id => note %></td>
    <td><%= note.updated_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
  </tr>
  <% end %>
</tbody>
</table>

